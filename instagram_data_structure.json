  "database_schema_suggestions": {
    "profiles": {
      "description": "Store Instagram user/profile information",
      "fields": {
        "id": "PRIMARY KEY - Instagram user ID",
        "username": "UNIQUE - Instagram username",
        "full_name": "User's display name",
        "is_verified": "Boolean - verification badge",
        "profile_pic_url": "Profile picture URL",
        "scraped_at": "Timestamp of when data was fetched",
        "created_at": "Database record creation time",
        "updated_at": "Last update time"
      }
    },
    "posts": {
      "description": "Store Instagram posts",
      "fields": {
        "id": "PRIMARY KEY - Instagram post ID",
        "short_code": "Short code for URL",
        "type": "Post type (Image/Video/Sidecar)",
        "caption": "TEXT - Post caption",
        "url": "Post URL",
        "display_url": "Main image/video URL",
