        "position": "Image order in carousel",
        "created_at": "Database record creation"
      }
    },
    "comments": {
      "description": "Store post comments",
      "fields": {
        "id": "PRIMARY KEY - Instagram comment ID",
        "post_id": "FOREIGN KEY -> posts(id)",
        "owner_id": "FOREIGN KEY -> profiles(id)",
        "text": "TEXT - Comment text",
        "likes_count": "Number of likes",
        "replies_count": "Number of replies",
        "timestamp": "Comment creation time",
        "parent_comment_id": "FOREIGN KEY -> comments(id) - For nested replies",
