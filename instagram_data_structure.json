      }
    },
    "post_hashtags": {
      "description": "Many-to-many relationship between posts and hashtags",
      "fields": {
        "post_id": "FOREIGN KEY -> posts(id)",
        "hashtag_id": "FOREIGN KEY -> hashtags(id)",
        "PRIMARY KEY": "(post_id, hashtag_id)"
